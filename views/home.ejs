<html>
    <head>
        <title>
            <%=title%>

        </title>
        <script src="https://kit.fontawesome.com/fb2f2264aa.js" crossorigin="anonymous"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
        <link rel="stylesheet"  href="/css/home.css">
     </head>
    <body >
        <i class="far fa-address-card"></i> <h1 >Contacts</h1>
        
        <div id ="outer" >
        
        <% for(let i of lol){%>
        <div id ="inner-<%= i._id %>"  >
           <a class="delete-contact-button" href="/delete-contact/?id=<%= i._id %>" ><i class="far fa-window-close"></i></a>
             <%=i.name%> <br>
            <%=i.password%> 
            
        </div>
        <br>
        <br>

            
        <%}%> 
    
        <form id="post-submit" action="/contact" method="POST">
            <input type="text" name="name" placeholder="name">
            <input type="text" name="password" placeholder="contact number">
            <button type="submit">submit</button>
        </form>
        
        
    </div>
    <script src="/js/home.js"></script>
    </body>
</html>